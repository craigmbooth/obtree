# Docker Compose Environment Variables
# Copy this to .env for local Docker development

# PostgreSQL Configuration
POSTGRES_DB=obtree
POSTGRES_USER=obtree_user
POSTGRES_PASSWORD=obtree_password
POSTGRES_PORT=5432

# Application Port (external)
APP_PORT=8000

# Security
SECRET_KEY=dev-secret-key-change-in-production-minimum-32-characters-required
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Application
APP_NAME=OBTree
DEBUG=True

# Invite Settings
INVITE_EXPIRATION_DAYS=7
